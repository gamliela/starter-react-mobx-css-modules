{
  "compilerOptions": {
    "experimentalDecorators": true,
    "sourceMap": true,
    "jsx": "react",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "lib": ["dom"],
    "allowJs": false,
    "skipLibCheck": false,
    "strict": true,
    "outDir": "out_tsc"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
